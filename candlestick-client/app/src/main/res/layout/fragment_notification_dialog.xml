<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="onGoClick"
    tools:context="io.nubhub.candlestick.NotificationDialogFragment">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="244dp"
        android:text="title-placeholder"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="391dp" />

    <TextView
        android:id="@+id/textViewSubject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:text="Subject- Placeholder"
        android:textSize="48dp"
        app:layout_constraintTop_toBottomOf="@+id/title"
        tools:layout_editor_absoluteX="227dp" />

    <TextView
        android:id="@+id/textViewDescription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:text="body-placeholder"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="391dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
